# CannTech Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Required for AI Debugging
ANTHROPIC_API_KEY=your_api_key_here

# Optional: Set to 'development' for more verbose logging
ENVIRONMENT=development

# Optional: Customize the AI model used for debugging
# Options: claude-3-haiku-20240307 (fast/cheap), claude-3-sonnet-20240229 (balanced)
AI_DEBUG_MODEL=claude-3-haiku-20240307